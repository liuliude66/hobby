<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBarView">
        <attr name="textLeft" format="string" />
        <attr name="textTitle" format="string" />
        <attr name="textRight" format="string" />
    </declare-styleable>
    <declare-styleable name="BottomBarView">
        <attr name="textNav1" format="string" />
        <attr name="textNav2" format="string" />
        <attr name="textNav3" format="string" />
        <attr name="textNav4" format="string" />
        <attr name="textNav5" format="string" />
    </declare-styleable>
    <declare-styleable name="TopBar">
        <attr name="left_text" format="string" />
        <attr name="title_text" format="string" />
        <attr name="right_text" format="string" />
        <attr name="left_margin" format="dimension" />
        <attr name="right_margin" format="dimension" />
        <attr name="left_txt_color" format="color" />
        <attr name="title_txt_color" format="color" />
        <attr name="right_txt_color" format="color" />
    </declare-styleable>
</resources>
